{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wdl/custom-title-4",
	"version": "1.0.0",
	"title": "見出しタイトル 04",
	"category": "lw-heading",
	"icon": "editor-textcolor",
	"description": "メインタイトルとサブタイトルを表示する見出しブロック（マージン調整可能）",
	"supports": {
		"anchor": true
	},
	"attributes": {
		"mainTitle": {
			"type": "string",
			"default": "タイトル"
		},
		"subTitle": {
			"type": "string",
			"default": "サブタイトル"
		},
		"headingLevel": {
			"type": "number",
			"default": 2
		},
		"subMarginBottomPc": {
			"type": "number",
			"default": 0.6
		},
		"subMarginBottomSp": {
			"type": "number",
			"default": 0.6
		}
	},
	"editorScript": "file:./custom-title-4.js",
	"editorStyle": [
		"file:./editor.css",
		"file:../../../assets/css/font_style.min.css",
		"file:../../../assets/css/editor_block_side.min.css"
	],
	"style": "file:./style.css",
	"no": 4
}