		"fontLiMain": {
			"type": "string",
			"default": ""
		},
		"fontWeightLiMain": {
			"type": "string",
			"default": "700"
		},
		"columnClass": {
			"type": "string",
			"default": "clm_3"
		},
		"mainAlign": {
			"type": "string",
			"default": "left"
