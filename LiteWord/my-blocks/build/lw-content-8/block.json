		"showButton": {
			"type": "boolean",
			"default": true
		},
		"contents": {
			"type": "array",
			"source": "query",
			"selector": ".lw-content-8__li",
			"query": {
				"sub": {
					"type": "string",
					"source": "html",
					"selector": ".ttl .sub"
				},
				"main": {
					"type": "string",
					"source": "html",
					"selector": ".ttl .main"
				},
				"text": {
					"type": "string",
					"source": "html",
					"selector": ".lw-content-8__text"
				},
				"url": {
					"type": "string",
					"source": "attribute",
					"selector": ".link",
					"attribute": "href",
					"default": ""
				},
				"btnText": {
					"type": "string",
					"source": "html",
					"selector": ".btn",
