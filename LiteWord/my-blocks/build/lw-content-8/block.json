		"fontWeightLi": {
			"type": "string",
			"default": "600"
		},
		"titleFontSizeClass": {
			"type": "string",
			"default": "font_size_m"
		},
		"fontLiP": {
			"type": "string",
			"default": ""
		},
		"fontColorLiP": {
			"type": "string",
			"default": "var(--color-black)"
		},
		"fontWeightLiP": {
			"type": "string",
			"default": "400"
		},
		"textFontSizeClass": {
			"type": "string",
			"default": "font_size_m"
		},
		"titleTag": {
			"type": "string",
