{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wdl/custom-title-2",
	"version": "1.0.0",
	"title": "見出しタイトル 02",
	"category": "lw-heading",
	"icon": "editor-textcolor",
	"description": "メインタイトルとサブタイトルを表示する見出しブロック（位置調整可能）",
	"supports": {
		"anchor": true
	},
	"attributes": {
		"mainTitle": {
			"type": "string",
			"default": "CONTENT"
		},
		"subTitle": {
			"type": "string",
			"default": "SUB TITLE TEXT"
		},
		"textAlignment": {
			"type": "string",
			"default": "left"
		},
		"headingLevel": {
			"type": "number",
			"default": 2
		}
	},
	"editorScript": "file:./custom-title-2.js",
	"editorStyle": [
		"file:./editor.css",
		"file:../../../assets/css/font_style.min.css",
		"file:../../../assets/css/editor_block_side.min.css"
	],
	"style": "file:./style.css",
	"no": 2
}